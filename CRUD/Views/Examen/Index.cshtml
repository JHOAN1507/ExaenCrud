@model List<CRUD.Models.ViewModels.ListExamenViewModel>

@{
    ViewBag.Title = "Mis Datos";
}

<h2>@ViewBag.Title</h2>
<div class="row" style="text-align:right;padding:5px;">
    <a href="~/Examen/Nuevo" class="btn btn-primary">Nuevo </a>

</div>
<div class="row">
    
    <div class="col-md-12"></div>
        <table class="table" >
            <tr>
                <th>#</th>
                <th>Nombre</th>
                <th>ApellidoPaterno</th>
                <th>ApellidoMaterno</th>
                <th>Curp</th>

            </tr>
            @foreach (var oElemnto in Model)
            {
              <tr>

                <td>@oElemnto.idEmpleado</td>
                <td>@oElemnto.nombre</td>
                <td>@oElemnto.apellidoPaterno</td>
                <td>@oElemnto.apellidoMaterno</td>
                <td>@oElemnto.curp</td>
                <td>
                    <a class="btn btn-default" href="~/Examen/Editar/@oElemnto.idEmpleado">Editar</a>
                    <a class="btn btn-danger" href="~/Examen/Eliminar/@oElemnto.idEmpleado">Eliminar</a>
                </td>
            </tr>

            }

        </table>
</div>

